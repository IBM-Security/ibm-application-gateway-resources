{
  "name": "iag-configurator",
  "version": "20.12",
  "homepage": "./",
  "description": "An online utility to create an <APPLICATION_NAME> file",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write"
  },
  "dependencies": {
    "ace-builds": "1.4.12",
    "apollo-boost": "0.4.2",
    "carbon-components": "10.24.0",
    "carbon-components-react": "7.24.0",
    "carbon-icons": "7.0.7",
    "graphql": "14.3.1",
    "js-yaml": "4.0.0",
    "react": "16.8.6",
    "react-ace": "9.1.4",
    "react-apollo": "2.5.6",
    "react-collapsible": "2.8.1",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "react-tooltip": "4.2.11",
    "react-web-tabs": "1.0.1"
  },
  "devDependencies": {
    "node-sass": "4.12.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
